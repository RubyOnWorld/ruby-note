# -*- coding: utf-8 -*-

num = 81

6.times do 
  puts "#{num.class}(#{num.size}): #{num}"
  num *= num
end


#区间作为序列
digits = 0..9
puts digits.include?(5)
putsdigits.min()
puts digits.max()
putsdigits.reject { |i| i < 5 }

#区间作为条件
f = File.open('block.rb', 'r')
while line = f.gets()
  puts line if line =~ /def/ .. line =~ /end/
end
f.close()

